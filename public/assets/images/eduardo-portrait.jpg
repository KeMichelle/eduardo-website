<!-- Eduardo Portrait Placeholder -->
<svg width="400" height="500" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="400" height="500" fill="#f1f5f9"/>
  
  <!-- Portrait Silhouette -->
  <circle cx="200" cy="180" r="80" fill="#e2e8f0"/>
  <rect x="120" y="240" width="160" height="200" rx="20" fill="#e2e8f0"/>
  
  <!-- Face Features -->
  <circle cx="180" cy="160" r="8" fill="#94a3b8"/>
  <circle cx="220" cy="160" r="8" fill="#94a3b8"/>
  <path d="M 180 200 Q 200 220 220 200" stroke="#94a3b8" stroke-width="3" fill="none"/>
  
  <!-- Professional Attire -->
  <rect x="160" y="260" width="80" height="100" fill="#1e293b"/>
  <rect x="180" y="280" width="40" height="80" fill="#ffffff"/>
  <rect x="195" y="280" width="10" height="80" fill="#0033A0"/>
  
  <!-- Background Elements -->
  <rect x="50" y="50" width="300" height="200" fill="url(#grad)" opacity="0.1"/>
  
  <!-- Gradient Definition -->
  <defs>
    <linearGradient id="grad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#FFD500;stop-opacity:0.3" />
      <stop offset="50%" style="stop-color:#0033A0;stop-opacity:0.2" />
      <stop offset="100%" style="stop-color:#EF3340;stop-opacity:0.3" />
    </linearGradient>
  </defs>
  
  <!-- Title -->
  <text x="200" y="450" text-anchor="middle" font-family="Arial, sans-serif" font-size="16" font-weight="bold" fill="#0033A0">Eduardo</text>
  <text x="200" y="470" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#666">Founder & CEO</text>
  <text x="200" y="485" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#999">EDUP Global Flex</text>
</svg>